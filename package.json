{
    "name": "powerhooks",
    "version": "0.0.1",
    "description": "A collection of powerfull react hooks",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/powerhooks.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "node dist/test/",
        "build": "tsc",
        "enable_short_import_path": "npm run build && denoify_enable_short_npm_import_path",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/garronej/powerhooks",
    "devDependencies": {
        "@types/memoizee": "^0.4.5",
        "@types/node": "^10.0.0",
        "@types/react": "^17.0.2",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "denoify": "^0.5.10",
        "eslint": "^7.7.0",
        "evt": "beta",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.2",
        "react": "^17.0.1",
        "typescript": "^4.1.5"
    },
    "dependencies": {
        "memoizee": "^0.4.15",
        "resize-observer-polyfill": "^1.5.1"
    }
}
